<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>test</display-name>
	<welcome-file-list>
		<welcome-file>browseIdex</welcome-file>
	</welcome-file-list>
	
	<!-- 配置资源地图   设置页面中请求哪个资源  去找哪个控制器
	
	  url-pattern 就是action中的值
	  servlet-name  这只处理的控制器名称   就像JFrame ActionListener实现类
	  servlet-class 就是具有控制器功能java类
	-->
	<servlet>
	    <servlet-name>checkOrderById</servlet-name>
	    <servlet-class>com.ccuix.eshop.controller.MemberFindorderByIdController</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>checkOrderById</servlet-name>
	    <url-pattern>/checkOrderById</url-pattern>
	</servlet-mapping>
	
	<servlet>
	    <servlet-name>orderList</servlet-name>
	    <servlet-class>com.ccuix.eshop.controller.MemberFindorderListController</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>orderList</servlet-name>
	    <url-pattern>/orderList</url-pattern>
	</servlet-mapping>
	
	<servlet>
	    <servlet-name>submitCart</servlet-name>
	    <servlet-class>com.ccuix.eshop.controller.MemberSubmitCartController</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>submitCart</servlet-name>
	    <url-pattern>/submitCart</url-pattern>
	</servlet-mapping>
	
	<servlet>
	    <servlet-name>findMerByCateAndLikeName</servlet-name>
	    <servlet-class>com.ccuix.eshop.controller.SelectByKeyController</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>findMerByCateAndLikeName</servlet-name>
	    <url-pattern>/findMerByCateAndLikeName</url-pattern>
	</servlet-mapping>
